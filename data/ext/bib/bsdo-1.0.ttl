@prefix : <http://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Atlas a rdfs:Class ;
    rdfs:label "Atlas"@en ;
    :isPartOf <http://bib.schema.org> ;
    :source <http://www.productontology.org/id/Atlas> ;
    rdfs:comment "A collection or bound volume of maps, charts, plates or tables, physical or in media form illustrating any subject."@en ;
    rdfs:subClassOf :CreativeWork .

:Audiobook a rdfs:Class ;
    rdfs:label "Audiobook"@en ;
    :isPartOf <http://bib.schema.org> ;
    rdfs:comment "An audiobook."@en ;
    rdfs:subClassOf :AudioObject,
        :Book .

:Chapter a rdfs:Class ;
    rdfs:label "Chapter"@en ;
    :isPartOf <http://bib.schema.org> ;
    rdfs:comment "One of the sections into which a book is divided. A chapter usually has a section number or a name."@en ;
    rdfs:subClassOf :CreativeWork .

:Collection a rdfs:Class ;
    rdfs:label "Collection"@en ;
    :isPartOf <http://bib.schema.org> ;
    rdfs:comment "A collection of items e.g. creative works or products."@en ;
    rdfs:subClassOf :CreativeWork .

:Newspaper a rdfs:Class ;
    rdfs:label "Newspaper"@en ;
    :isPartOf <http://bib.schema.org> ;
    :source <http://www.productontology.org/id/Newspaper> ;
    rdfs:comment "A publication containing information about varied topics that are pertinent to general information, a geographic area, or a specific subject matter (i.e. business, culture, education). Often published daily."@en ;
    rdfs:subClassOf :Periodical .

:Thesis a rdfs:Class ;
    rdfs:label "Thesis"@en ;
    :isPartOf <http://bib.schema.org> ;
    :source <http://www.productontology.org/id/Thesis> ;
    rdfs:comment "A thesis or dissertation document submitted in support of candidature for an academic degree or professional qualification."@en ;
    rdfs:subClassOf :CreativeWork .

<file:///Users/wallisr/Development/Schema/ttl/schemaorg/data/ext/bib/bsdo-1.0.rdfa> :softwareVersion "bib.schema.org Version 0.1"@en .

:abridged a rdf:Property ;
    rdfs:label "abridged"@en ;
    :domainIncludes :Book ;
    :isPartOf <http://bib.schema.org> ;
    :rangeIncludes :Boolean ;
    rdfs:comment "Indicates whether the book is an abridged edition."@en .

:duration a rdf:Property ;
    :domainIncludes :Audiobook .

:inSupportOf a rdf:Property ;
    rdfs:label "inSupportOf"@en ;
    :domainIncludes :Thesis ;
    :isPartOf <http://bib.schema.org> ;
    :rangeIncludes :Text ;
    rdfs:comment "Qualification, candidature, degree, application that Thesis supports."@en .

:pageEnd a rdf:Property ;
    :domainIncludes :Chapter .

:pageStart a rdf:Property ;
    :domainIncludes :Chapter .

:pagination a rdf:Property ;
    :domainIncludes :Chapter .

:publishedBy a rdf:Property ;
    rdfs:label "publishedBy"@en ;
    :domainIncludes :PublicationEvent ;
    :isPartOf <http://bib.schema.org> ;
    :rangeIncludes :Organization,
        :Person ;
    rdfs:comment "An agent associated with the publication event."@en .

:readBy a rdf:Property ;
    rdfs:label "readBy"@en ;
    :domainIncludes :Audiobook ;
    :isPartOf <http://bib.schema.org> ;
    :rangeIncludes :Person ;
    rdfs:comment "A person who reads (performs) the audiobook."@en ;
    rdfs:subPropertyOf :actor .

:translationOfWork a rdf:Property ;
    rdfs:label "translationOfWork"@en ;
    :domainIncludes :CreativeWork ;
    :inverseOf :workTranslation ;
    :isPartOf <http://bib.schema.org> ;
    :rangeIncludes :CreativeWork ;
    rdfs:comment "The work that this work has been translated from. e.g. 物种起源 is a translationOf “On the Origin of Species”"@en .

:workTranslation a rdf:Property ;
    rdfs:label "workTranslation"@en ;
    :domainIncludes :CreativeWork ;
    :inverseOf :translationOfWork ;
    :isPartOf <http://bib.schema.org> ;
    :rangeIncludes :CreativeWork ;
    rdfs:comment "A work that is a translation of the content of this work. e.g. 西遊記 has an English workTranslation “Journey to the West”,a German workTranslation “Monkeys Pilgerfahrt” and a Vietnamese  translation Tây du ký bình khảo."@en .



    @prefix : <http://schema.org/> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

    :ComicCoverArt a rdfs:Class ;
        rdfs:label "ComicCoverArt"@en ;
        :category "Comics"@en ;
        :isPartOf <http://bib.schema.org> ;
        rdfs:comment "The artwork on the cover of a comic."@en ;
        rdfs:subClassOf :ComicStory,
            :CoverArt .

    :ComicIssue a rdfs:Class ;
        rdfs:label "ComicIssue"@en ;
        :category "Comics"@en ;
        :isPartOf <http://bib.schema.org> ;
        rdfs:comment """Individual comic issues are serially published as
    	part of a larger series. For the sake of consistency, even one-shot issues
    	belong to a series comprised of a single issue. All comic issues can be
    	uniquely identified by: the combination of the name and volume number of the
    	series to which the issue belongs; the issue number; and the variant
    	description of the issue (if any)."""@en ;
        rdfs:subClassOf :PublicationIssue .

    :ComicSeries a rdfs:Class ;
        rdfs:label "ComicSeries"@en ;
        :category "Comics"@en ;
        :isPartOf <http://bib.schema.org> ;
        rdfs:comment """A sequential publication of comic stories under a
    	unifying title, for example "The Amazing Spider-Man" or "Groo the
    	Wanderer"."""@en ;
        rdfs:subClassOf :Periodical .

    :ComicStory a rdfs:Class ;
        rdfs:label "ComicStory"@en ;
        :category "Comics"@en ;
        :isPartOf <http://bib.schema.org> ;
        rdfs:comment """The term "story" is any indivisible, re-printable
    	unit of a comic, including the interior stories, covers, and backmatter. Most
    	comics have at least two stories: a cover (ComicCoverArt) and an interior story."""@en ;
        rdfs:subClassOf :CreativeWork .

    :CoverArt a rdfs:Class ;
        rdfs:label "CoverArt"@en ;
        :category "Comics"@en ;
        :isPartOf <http://bib.schema.org> ;
        rdfs:comment "The artwork on the outer surface of a CreativeWork."@en ;
        rdfs:subClassOf :VisualArtwork .

    <file:///Users/wallisr/Development/Schema/ttl/schemaorg/data/ext/bib/comics.rdfa> :softwareVersion "bib extension - version 0.1"@en .

    :GraphicNovel a :BookFormatType ;
        rdfs:label "GraphicNovel"@en ;
        :category "Comics"@en ;
        :isPartOf <http://bib.schema.org> ;
        rdfs:comment "Book format: GraphicNovel. May represent a bound collection of ComicIssue instances."@en .

    :artist a rdf:Property ;
        rdfs:label "artist"@en ;
        :category "Comics"@en ;
        :domainIncludes :ComicIssue,
            :ComicStory,
            :VisualArtwork ;
        :isPartOf <http://bib.schema.org> ;
        :rangeIncludes :Person ;
        rdfs:comment """The primary artist for a work
    	in a medium other than pencils or digital line art--for example, if the
    	primary artwork is done in watercolors or digital paints."""@en .

    :colorist a rdf:Property ;
        rdfs:label "colorist"@en ;
        :category "Comics"@en ;
        :domainIncludes :ComicIssue,
            :ComicStory,
            :VisualArtwork ;
        :isPartOf <http://bib.schema.org> ;
        :rangeIncludes :Person ;
        rdfs:comment "The individual who adds color to inked drawings."@en .

    :inker a rdf:Property ;
        rdfs:label "inker"@en ;
        :category "Comics"@en ;
        :domainIncludes :ComicIssue,
            :ComicStory,
            :VisualArtwork ;
        :isPartOf <http://bib.schema.org> ;
        :rangeIncludes :Person ;
        rdfs:comment "The individual who traces over the pencil drawings in ink after pencils are complete."@en .

    :letterer a rdf:Property ;
        rdfs:label "letterer"@en ;
        :category "Comics"@en ;
        :domainIncludes :ComicIssue,
            :ComicStory,
            :VisualArtwork ;
        :isPartOf <http://bib.schema.org> ;
        :rangeIncludes :Person ;
        rdfs:comment "The individual who adds lettering, including speech balloons and sound effects, to artwork."@en .

    :penciler a rdf:Property ;
        rdfs:label "penciler"@en ;
        :category "Comics"@en ;
        :domainIncludes :ComicIssue,
            :ComicStory,
            :VisualArtwork ;
        :isPartOf <http://bib.schema.org> ;
        :rangeIncludes :Person ;
        rdfs:comment "The individual who draws the primary narrative artwork."@en .

    :publisherImprint a rdf:Property ;
        rdfs:label "publisherImprint"@en ;
        :category "Comics"@en ;
        :domainIncludes :CreativeWork ;
        :isPartOf <http://bib.schema.org> ;
        :rangeIncludes :Organization ;
        rdfs:comment "The publishing division which published the comic."@en .

    :variantCover a rdf:Property ;
        rdfs:label "variantCover"@en ;
        :category "Comics"@en ;
        :domainIncludes :ComicIssue ;
        :isPartOf <http://bib.schema.org> ;
        :rangeIncludes :Text ;
        rdfs:comment """A description of the variant cover
    	for the issue, if the issue is a variant printing. For example, "Bryan Hitch
    	Variant Cover" or "2nd Printing Variant"."""@en .
